<body>
    <div class="col m-h-3">

        <input id="search" class="miro-input miro-input--primary miro-input--small m-v-5" type="text" placeholder="Search by name">
        <div class="miro-h3 m-b-3 m-t-2">Select from your dices:</div>
        {{!-- {{#each plugins}} --}}
            {{!-- <a class="dropdown-item" href="/admin/paykeys?pluginName={{this.name}}">{{this.name}}</a> --}}
            <div class="dice m-b-2">
                    <div class="row" style="max-width: 100%;">
                        <img class="expand-button miro-btn miro-btn--secondary" src="/static/svg/menu.svg" alt="">
                        <img class="place-button miro-btn miro-btn--secondary" src="/static/svg/arrow.svg" alt="">
                        <span class="name m-l-5">Dice name #1111111111111111111111111111111111111111111111111</span>
                        <img class="delete-button miro-btn miro-btn--secondary" src="/static/svg/cross.svg" alt="">
                    </div>
                    <div class="row content">
                        Some content
                    </div>
            </div>
            <div class="dice m-b-2">
                    <div class="row" style="max-width: 100%;">
                        <img class="expand-button miro-btn miro-btn--secondary" src="/static/svg/menu.svg" alt="">
                        <img class="place-button miro-btn miro-btn--secondary" src="/static/svg/arrow.svg" alt="">
                        <span class="name m-l-5">Dice name #2</span>
                        <img class="delete-button miro-btn miro-btn--secondary" src="/static/svg/cross.svg" alt="">
                    </div>
                    <div class="row content">
                        Some content
                    </div>
            </div>
        {{!-- {{/each}} --}}
        {{!-- add button --}}
        <img class="expand-button col miro-btn miro-btn--secondary m-t-3" src="/static/svg/add.svg" alt="">
        <form method="POST" class="hidden">
            {{!-- adding form --}}
            <input type="text" class="miro-input miro-input--primary miro-input--small" name="name">
            <textarea type="text" class="miro-input miro-input--primary miro-input--small" style="height: 150px !important;" name="data"></textarea>
            <input type="submit" class="miro-btn miro-btn--small">
        </form>
    </div>
